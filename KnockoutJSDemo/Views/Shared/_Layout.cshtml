<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link rel="shortcut icon" href="@Url.Content("~/Content/images/favicon.ico")" />
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css"  media="screen" />
    <link href="@Url.Content("~/Content/themes/base/jquery.ui.all.css")" rel="stylesheet" type="text/css" />
</head>
<body>
<div class="header-container">
    <div class="logo"><a href="@Url.Action("Index", "Home")">Knockout JS Demo</a></div>
    <div class="clear"></div>
</div>
<div class="poster-container-no-image">
    <div class="poster-inner"></div>
</div>
<div class="nav-main">
    <ul>
        <li class="first">@Html.ActionLink("Home", "Index", "Home") </li>
        <li>@Html.ActionLink("About", "About", "Home") </li>
    </ul>
</div>
<div class="content-container">
    <div class="content-container-inner">
        <div class="content-main">
            @RenderBody()
        </div>
		<div class="content-left">
            <div class="side-bucket">
                <ul>
                   <li> @Html.NavLink("Hello World", "HelloWorld", "Example")</li>
                    <li>@Html.NavLink("Dependent Observable - Short", "DependentObservable", "Example")</li>
                    <li>@Html.NavLink("Dependent Observable - Long", "DependentObservableLong", "Example")</li>
                    <li>@Html.NavLink("ASP.NET MVC Integration", "Mvc", "Example")</li>
                    <li>@Html.NavLink("Templating", "Templating", "Example")</li>
                    <li>@Html.NavLink("Cascading Drop Down", "Cascading", "Example")</li>
                    <li>@Html.NavLink("Custom Binding", "CustomBinding", "Example")</li>
                    <li>@Html.NavLink("JQuery Integration", "JQueryIntegration", "Example")</li>
                    <li>@Html.NavLink("Google Maps Integration", "GoogleMaps", "Example")</li>
                    <li>@Html.NavLink("Testing", "Testing", "Example")</li>
                    <li>@Html.NavLink("To Do List Example","ToDoList","Example")</li>
                    <li>@Html.ActionLink("Beta - Control Flow Bindings", "ControlFlowBindings", "KoBeta") </li>
                    <li>@Html.ActionLink("Beta - With Binding", "WithBinding", "KoBeta") </li>
                    <li>@Html.ActionLink("Beta - Binding Providers", "BindingProviders", "KoBeta") </li>
                </ul>
            </div>
        </div>  
        <div class="clear"></div>
    </div>
</div>
<div class="footer">
    <div class="nav-footer">
        <p class="copyright">MVVM Rocks!</p>
    </div>
</div>
<script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.tmpl.min.js")"  type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery-ui-1.8.11.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/knockout-1.3.0beta.js")" type="text/javascript"></script>
@RenderSection("JavaScript", false)
</body>
</html>

